  <system.webServer>
    <httpProtocol>
      <customHeaders>
        <add name="Referrer-Policy" value="strict-origin-when-cross-origin" />
        <add name="Strict-Transport-Security" value="max-age=31536000; includeSubDomains" />
        <add name="X-Content-Type-Options" value="nosniff" />
        <add name="X-Frame-Options" value="SAMEORIGIN" />
        <add name="X-XSS-Protection" value="1; mode=block" />
      </customHeaders>
    </httpProtocol>
    <validation validateIntegratedModeConfiguration="false" />
    <staticContent>
      <remove fileExtension=".eot" />
      <mimeMap fileExtension=".eot" mimeType="application/vnd.ms-fontobject" />
      <remove fileExtension=".ttf" />
      <mimeMap fileExtension=".ttf" mimeType="application/octet-stream" />
      <remove fileExtension=".woff" />
      <mimeMap fileExtension=".woff" mimeType="application/octet-stream" />
      <remove fileExtension=".svg" />
      <mimeMap fileExtension=".svg" mimeType="image/svg+xml" />
      <remove fileExtension=".webp" />
      <mimeMap fileExtension=".webp" mimeType="image/webp" />
      <remove fileExtension=".json" />
      <mimeMap fileExtension=".json" mimeType="application/json" />
    </staticContent>
    <handlers>
      <remove name="ExtensionlessUrlHandler-ISAPI-4.0_32bit" />
      <remove name="ExtensionlessUrlHandler-ISAPI-4.0_64bit" />
      <remove name="ExtensionlessUrlHandler-Integrated-4.0" />
      <add name="ExtensionlessUrlHandler-ISAPI-4.0_32bit" path="*." verb="GET,HEAD,POST,DEBUG,PUT,DELETE,PATCH,OPTIONS" modules="IsapiModule" scriptProcessor="%windir%\Microsoft.NET\Framework\v4.0.30319\aspnet_isapi.dll" preCondition="classicMode,runtimeVersionv4.0,bitness32" responseBufferLimit="0" />
      <add name="ExtensionlessUrlHandler-ISAPI-4.0_64bit" path="*." verb="GET,HEAD,POST,DEBUG,PUT,DELETE,PATCH,OPTIONS" modules="IsapiModule" scriptProcessor="%windir%\Microsoft.NET\Framework64\v4.0.30319\aspnet_isapi.dll" preCondition="classicMode,runtimeVersionv4.0,bitness64" responseBufferLimit="0" />
      <add name="ExtensionlessUrlHandler-Integrated-4.0" path="*." verb="GET,HEAD,POST,DEBUG,PUT,DELETE,PATCH,OPTIONS" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
      <add name="CustomAccessibilityHanlder" path="accessibility/*" verb="GET" type="System.Web.Handlers.TransferRequestHandler" preCondition="integratedMode,runtimeVersionv4.0" />
    </handlers>
    <rewrite>
      <rules>
        <rule name="LowerCaseRule" stopProcessing="true">
          <match url="[A-Z]" ignoreCase="false" />
          <action type="Redirect" url="{ToLower:{URL}}" redirectType="Permanent" />
          <conditions>
            <add input="{REQUEST_FILENAME}" matchType="IsFile" negate="true" />
            <add input="{HTTP_URL}" pattern="^/aspnet-mvc/api|^/production-aspnet-mvc|^/api" negate="true" />
            <add input="{HTTP_URL}" pattern="^/aspnet-mvc/(html5-dashboard-sample-app|webmail|tripxpert|olympic-games|admin-dashboard)" negate="true" />
          </conditions>
        </rule>
        <rule name="LowerCaseRule2" stopProcessing="true">
          <match url="(.*)" />
          <action type="Redirect" url="{ToLower:{URL}}" redirectType="Permanent" />
          <conditions>
            <add input="{URL}" pattern="[A-Z]" ignoreCase="false" />
            <add input="{HTTP_URL}" pattern="^/aspnet-mvc/api|^/production-aspnet-mvc/api|^/api" negate="true" />
            <add input="{HTTP_URL}" pattern="^/aspnet-mvc/(html5-dashboard-sample-app|webmail|tripxpert|olympic-games|admin-dashboard)" negate="true" />
          </conditions>
        </rule>
        <rule name="AddSlash" stopProcessing="true">
          <match url="(.*)" ignoreCase="true" />
          <action type="Redirect" url="/{C:1}/" redirectType="Permanent" />
          <conditions>
            <add input="{URL}" pattern="(production-aspnet-mvc|aspnet-mvc)$" />
          </conditions>
        </rule>
        <rule name="RemoveHome" stopProcessing="true">
          <match url="(.*)" ignoreCase="true" />
          <action type="Redirect" url="/{C:1}/" redirectType="Permanent" />
          <conditions>
            <add input="{URL}" pattern="(production-aspnet-mvc|aspnet-mvc)/home(/index)?" />
            <add input="{URL}" pattern="(production-aspnet-mvc|aspnet-mvc)/home(/SitemapXml)?" negate="true" />
          </conditions>
        </rule>
        <rule name="RemoveSlash" stopProcessing="true">
          <match url="(.*)/$" />
          <action type="Redirect" url="{R:1}" redirectType="Permanent" />
          <conditions>
            <add input="{URL}" pattern="(production-aspnet-mvc|aspnet-mvc)$" />
            <add input="{HTTP_URL}" pattern="^/aspnet-mvc/api|^/production-aspnet-mvc/api|^/api" negate="true" />
            <add input="{HTTP_URL}" pattern="^/aspnet-mvc/(html5-dashboard-sample-app|webmail|tripxpert|olympic-games|admin-dashboard)" negate="true" />
          </conditions>
        </rule>
        <rule name="RemoveIndex" stopProcessing="true">
          <match url="(.*)/index/?$" />
          <action type="Redirect" url="{R:1}" redirectType="Permanent" />
        </rule>
        <rule name="GridKinvey">
          <match url="^grid/everlive" ignoreCase="true" />
          <action type="Redirect" url="grid/kinvey" redirectType="Found" />
        </rule>
        <rule name="checkbox">
          <match url="^styling/checkboxes" ignoreCase="true" />
          <action type="Redirect" url="checkbox" redirectType="Found" />
        </rule>
        <rule name="radiobutton">
          <match url="^styling/radios" ignoreCase="true" />
          <action type="Redirect" url="radiobutton" redirectType="Found" />
        </rule>
        <rule name="GanttColumns">
          <match url="^gantt/column-resizing" ignoreCase="true" />
          <action type="Redirect" url="gantt/columns" redirectType="Found" />
        </rule>
        <rule name="TimelineSharedDatasource">
          <match url="^timeline/shared$" ignoreCase="true" />
          <action type="Redirect" url="timeline/shared-datasource" redirectType="Found" />
        </rule>
        <rule name="GridPagerFunctionality">
          <match url="^grid/pager-visibility$" ignoreCase="true" />
          <action type="Redirect" url="grid/pager-functionality" redirectType="Found" />
        </rule>
        <rule name="scheduler/keyboard-navigation">
          <match url="^scheduler/selection$" ignoreCase="true" />
          <action type="Redirect" url="scheduler/keyboard-navigation" redirectType="Found" />
        </rule>
        <rule name="ChatTravel">
          <match url="^chat/travel$" ignoreCase="true" />
          <action type="Redirect" url="chat/index" redirectType="Found" />
        </rule>
        <rule name="chatHealthcare" patternSyntax="ECMAScript" stopProcessing="true">
          <action type="Redirect" url="/{C:1}/chat" redirectType="Found" />
          <conditions>
            <add input="{HTTP_URL}" pattern="^/(.*)/chat/healthcare$" />
          </conditions>
        </rule>
        <rule name="Sitemap" stopProcessing="true">
          <match url="^sitemap.xml" ignoreCase="true" />
          <action type="Redirect" url="home/SitemapXml" redirectType="Found" />
        </rule>
        <rule name="beta-demos" stopProcessing="true">
          <match url="^beta/?(.*)$" />
          <action type="Redirect" url="/aspnet-mvc/{R:1}" redirectType="Permanent" />
        </rule>
        <rule name="grid/serverdetails">
          <match url="^grid/serverdetails$" ignoreCase="true" />
          <action type="Redirect" url="grid/index" appendQueryString="false" redirectType="Found" />
        </rule>
        <rule name="grid/serverhierarchy">
          <match url="^grid/serverhierarchy$" ignoreCase="true" />
          <action type="Redirect" url="grid/index" appendQueryString="false" redirectType="Found" />
        </rule>
      </rules>
    </rewrite>
    <httpErrors errorMode="Detailed" />
	  <security>
		  <requestFiltering>
			  <requestLimits maxAllowedContentLength="1073741824" />
		  </requestFiltering>
	  </security>
